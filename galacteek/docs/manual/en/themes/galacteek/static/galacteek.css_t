@import url("basic.css");

@font-face {
    font-family: 'Segoe UI';
    src: url('../../../../share/static/fonts/SegoeUI.woff2') format('woff2');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'Montserrat';
    font-style: normal;
    font-weight: 400;
    src: url('../../../../share/static/fonts/Montserrat-Regular.woff2') format('woff2');
}

@font-face {
    font-family: 'Inter UI';
    font-style:  normal;
    font-weight: 400;
    src: url('../../../../share/static/fonts/Inter-UI-Medium.woff2') format('woff2');
}
 
/* -- page layout ----------------------------------------------------------- */
 
body {
    font: 14 "Montserrat";
    background-color: #a4bfbb;
    color: #5f8e8c;
    margin: 0;
    padding: 0;
}

div.documentwrapper {
    float: left;
    width: 100%;
}

div.bodywrapper {
    margin: 0 0 0 {{ theme_sidebarwidth|todim }};
}

hr {
    border: 1px solid #B1B4B6;
}
 
div.document {
    background-color: #eee;
    background-color: #323232;
}
 
div.body {
    background-color: #323232;

    color: black;

    padding: 0 30px 30px 30px;
    font-size: 0.9em;
}
 
div.footer {
    color: #4d657c;
    width: 100%;
    padding: 13px 0;
    text-align: center;
    font-size: 75%;
}
 
div.footer a {
    color: #4d657c;
    text-decoration: underline;
}
 
div.related {
    background-color: #4a9ea1;
    line-height: 32px;
    color: #fff;
    text-shadow: 0px 1px 0 #4d657c;
    font-size: 0.9em;
    border-top: 1px solid grey;
    border-bottom: 2px solid grey;
}
 
div.related a {
    color: #E2F3CC;
}
 
div.sphinxsidebar {
    font-size: 0.75em;
    line-height: 1.5em;
}

div.sphinxsidebarwrapper{
    padding: 20px 0;
}
 
div.sphinxsidebar h3,
div.sphinxsidebar h4 {
    font-family: "Nobile", sans-serif;
    color: #222;
    font-size: 1.2em;
    font-weight: normal;
    margin: 0;
    padding: 5px 10px;
    background-color: #ddd;
    background-color: #2d4e69;
    color: white;
    /* text-shadow: 1px 1px 0 white; */
}

div.sphinxsidebar h4{
    font-size: 1.1em;
}
 
div.sphinxsidebar h3 a {
    color: #4d657c;
}
 
div.sphinxsidebar p {
    color: #888;
    padding: 5px 20px;
}
 
div.sphinxsidebar p.topless {
}
 
div.sphinxsidebar ul {
    margin: 10px 20px;
    padding: 0;
    color: #000;
}
 
div.sphinxsidebar a {
    color: #d7cac3;
    font-size: 150%;
}

div.sphinxsidebar a:hover {
    background-color: #2d4e69;
}
 
 
div.sphinxsidebar input {
    border: 1px solid #ccc;
    font-family: "Nobile", sans-serif;
    font-size: 1em;
}

div.sphinxsidebar .searchformwrapper {
    margin-left: 20px;
    margin-right: 20px;
}

/* -- body styles ----------------------------------------------------------- */
 
a {
    color: #446474;
    font-size: 125%;
    text-decoration: none;
}
 
a:hover {
    color: #446474;
    text-decoration: underline;
    background-color: #fca409;
}
 
div.body h1,
div.body h2,
div.body h3,
div.body h4,
div.body h5,
div.body h6 {
    font-family: "Nobile", sans-serif;
    font-weight: normal;
    color: #212224;
    margin: 25px 0px 20px 0px;
    padding: 5px 0 5px 10px;
    text-shadow: 0px 1px 0 white
    border: 1px solid white;
    border-radius: 8px;
}
 
div.body h1 {
    margin-top: 0;
    font-size: 200%;
    border-bottom: 2px solid black;
    margin-bottom: 20px;

    border-radius: 2px;
}

div.body h2 { font-size: 150%; background-color: #C8D5E3; }
div.body h3 { font-size: 120%; background-color: #D8DEE3; }
div.body h4 { font-size: 110%; background-color: #D8DEE3; }
div.body h5 { font-size: 100%; background-color: #D8DEE3; }
div.body h6 { font-size: 100%; background-color: #D8DEE3; }
 
a.headerlink {
    color: #c60f0f;
    font-size: 0.8em;
    padding: 0 4px 0 4px;
    text-decoration: none;
}
 
a.headerlink:hover {
    background-color: #c60f0f;
    color: white;
}
 
div.body p, div.body dd, div.body li {
    line-height: 1.5em;
}
 
div.admonition p.admonition-title + p {
    display: inline;
}

div.highlight{
    /* background-color: white; */
    background-color: #D8DEE3;
}

div.note {
    background-color: #eee;
    border: 1px solid #ccc;
}
 
div.seealso {
    background-color: #ffc;
    border: 1px solid #ff6;
}
 
div.topic {
    background-color: #eee;
}
 
div.warning {
    background-color: #ffe4e4;
    border: 1px solid #f66;
}
 
p.admonition-title {
    display: inline;
}
 
p.admonition-title:after {
    content: ":";
}
 
pre {
    padding: 10px;
    background-color: White;
    color: #222;
    line-height: 1.2em;
    border: 1px solid #C6C9CB;
    font-size: 1.1em;
    margin: 1.5em 0 1.5em 0;
    -webkit-box-shadow: 1px 1px 1px #d8d8d8;
    -moz-box-shadow: 1px 1px 1px #d8d8d8;
}
 
code {
    background-color: #ecf0f3;
    color: #222;
    /* padding: 1px 2px; */
    font-size: 1.1em;
    font-family: monospace;
}

.viewcode-back {
    font-family: Arial, sans-serif;
}

div.viewcode-block:target {
    background-color: #f4debf;
    border-top: 1px solid #ac9;
    border-bottom: 1px solid #ac9;
}

div.code-block-caption {
    background-color: #ddd;
    color: #222;
    border: 1px solid #C6C9CB;
}

div.body {
    border-left: 1px solid grey;
    border-right: 1px solid grey;
    border-bottom: 1px solid grey;
    border-top: 1px solid grey;
    border-radius: 6px;
    margin-top: 8px;
    margin-bottom: 5px;
    background-color: lightgrey;
}
